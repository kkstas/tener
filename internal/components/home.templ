package components

import (
	"context"

	"github.com/kkstas/tjener/internal/model"
)

templ Page(ctx context.Context, expenses []model.Expense, currencies []string, categories []model.ExpenseCategory) {
	@BaseHTML(ctx) {
		@CreateExpenseContainer(ctx, currencies, categories)
		@ExpensesDateRangePicker(ctx)
		@Expenses(ctx, expenses, currencies, categories)
	}
}
